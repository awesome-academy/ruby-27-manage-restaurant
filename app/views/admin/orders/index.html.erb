<div class="list">
  <h1><%= t "order.infor_kitchen" %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col"><%= "#" %></th>
      <th scope="col"><%= t "order.number_table" %></th>
      <th scope="col"><%= t "order.total_money" %></th>
      <th scope="col"><%= t "order.staff" %></th>
      <th scope="col"><%= t "order.status" %></th>
      <th scope="col"><%= t "order.action" %></th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "order", collection: @orders %>
  </tbody>
</table>
<%= paginate @orders, theme: "twitter-bootstrap-3" %>
